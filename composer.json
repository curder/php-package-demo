{
    "name": "curder/php-package-demo",
    "description": "A wonderful package to perform unit conversion in PHP",
    "keywords": [
        "curder",
        "php-package-demo"
    ],
    "homepage": "https://github.com/curder/php-package-demo",
    "license": "MIT",
    "authors": [
        {
            "name": "curder",
            "email": "q.curder@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Curder\\PhpPackageDemo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Curder\\PhpPackageDemo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
